		cp -df Output_x86_64/release/bin/RuntimeLibs/QT/libQt5Multimedia.so.5                   ${PKG_NAME}/RuntimeLibs/QT
		cp -df Output_x86_64/release/bin/RuntimeLibs/QT/libQt5Multimedia.so.5.5                 ${PKG_NAME}/RuntimeLibs/QT
		cp -df Output_x86_64/release/bin/RuntimeLibs/QT/libQt5Multimedia.so.5.5.1               ${PKG_NAME}/RuntimeLibs/QT
		cp -df Output_x86_64/release/bin/RuntimeLibs/QT/libQt5MultimediaWidgets.so              ${PKG_NAME}/RuntimeLibs/QT
		cp -df Output_x86_64/release/bin/RuntimeLibs/QT/libQt5MultimediaWidgets.so.5            ${PKG_NAME}/RuntimeLibs/QT
		cp -df Output_x86_64/release/bin/RuntimeLibs/QT/libQt5MultimediaWidgets.so.5.5          ${PKG_NAME}/RuntimeLibs/QT
