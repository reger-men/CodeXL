		cp -df Output_x86_64/release/bin/libCXLThreadProfileAPI.so              ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libCXLPowerProfileAPI.so               ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libboost_filesystem.so.1.59.0           ${PKG_NAME}
