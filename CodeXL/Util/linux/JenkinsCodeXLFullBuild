		cp -f Output_x86_64/release/bin/HTML/welcomepage_frame_analyze.png                                  ${PKG_NAME}/HTML/
		cp -f Output_x86_64/release/bin/HTML/welcomepage_frame_analyze_large.png                            ${PKG_NAME}/HTML/
