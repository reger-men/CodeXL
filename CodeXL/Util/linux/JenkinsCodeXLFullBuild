		cp -df Output_x86_64/release/bin/RuntimeLibs/QT/libQt5Core.so.5.5                       ${PKG_NAME}/RuntimeLibs/QT
		cp -df Output_x86_64/release/bin/RuntimeLibs/QT/libQt5Core.so.5.5.1                     ${PKG_NAME}/RuntimeLibs/QT
